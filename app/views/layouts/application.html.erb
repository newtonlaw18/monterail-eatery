    <script src="<%= @server_url %>/assets/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-bez/jquery.bez.min.js"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-actual/jquery.actual.min.js"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script type="text/javascript" src="<%= @server_url %>/assets/plugins/bootstrap-select2/select2.min.js"></script>
    <script type="text/javascript" src="<%= @server_url %>/assets/plugins/classie/classie.js"></script>
    <script src="<%= @server_url %>/assets/plugins/switchery/js/switchery.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-metrojs/MetroJs.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-isotope/isotope.pkgd.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/codrops-dialogFx/dialogFx.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/owl-carousel/owl.carousel.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-nouislider/jquery.nouislider.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>/assets/plugins/jquery-nouislider/jquery.liblink.js" type="text/javascript"></script>
    <!-- END VENDOR JS -->
    <!-- BEGIN CORE TEMPLATE JS -->
    <script src="<%= @server_url %>/pages/js/pages.min.js"></script>
    <!-- END CORE TEMPLATE JS -->
    <!-- BEGIN PAGE LEVEL JS -->





    
    <script src="<%= @server_url %>assets/plugins/modernizr.custom.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/plugins/bootstrapv3/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script src="/pages/js/pages.blank.js"></script>

    <script src="<%= @server_url %>assets/js/demo.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/js/scripts.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/js/notifications.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/js/gallery.js" type="text/javascript"></script>
    <script src="<%= @server_url %>assets/js/scripts.js" type="text/javascript"></script>
  </body>
</html>



